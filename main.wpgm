import wollok.game.*
import hector.*
import config.*
import cultivos.*


object juego {
    method iniciar() {
		game.addVisual(hector)
		config.configurarTeclas()
	}

}
program granjavilla {
	game.title("Granjavilla")
	game.height(20)
	game.width(20)
	game.boardGround("ground.png")
    juego.iniciar()
    game.showAttributes(hector) //Debug
	game.start()
}
